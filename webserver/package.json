{
  "name": "incuvision-webserver",
  "version": "1.0.0",
  "description": "webserver for Incuvision",
  "main": "src/server.js",
  "engines": {
    "node": "10.15.0",
    "npm": "6.9.0"
  },
  "dependencies": {
    "aws-sdk": "^2.437.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "history": "^4.9.0",
    "html": "^1.0.0",
    "jsonwebtoken": "^8.4.0",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.1",
    "pug": "^2.0.3",
    "sequelize": "^4.42.0",
    "socket.io": "^2.2.0",
    "ws": "^6.1.3"
  },
  "devDependencies": {
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "sass": "^1.17.3"
  },
  "scripts": {
    "test": "jest",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "server": "nodemon ./src/server.js",
    "start": "node ./src/server.js",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "nodemon": {
    "ignore": [
      "public/*"
    ]
  },
  "production": {
    "use_env_variable": "DATABASE_URL"
  },
  "author": "Reed Barnes",
  "license": "ISC"
}
